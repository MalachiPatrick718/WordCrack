{
  "name": "wordcrack",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "seed:puzzle-bank": "node scripts/seed-puzzle-bank.js --kind daily --file puzzle_bank.json",
    "seed:practice-puzzle-bank": "node scripts/seed-puzzle-bank.js --kind practice --file practice_puzzle_bank.json",
    "puzzle-bank:append": "node scripts/append-puzzle-bank.js --count 30",
    "puzzle-bank:append:dry": "node scripts/append-puzzle-bank.js --count 30 --dry-run",
    "puzzle-bank:replenish": "node scripts/replenish-puzzle-bank.js --count 30",
    "puzzle-bank:replenish:buffered": "node scripts/replenish-puzzle-bank.js --count 30 --min-buffer 1000"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@supabase/supabase-js": "^2.90.0",
    "expo": "~54.0.31",
    "expo-dev-client": "~6.0.20",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-iap": "^14.7.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
